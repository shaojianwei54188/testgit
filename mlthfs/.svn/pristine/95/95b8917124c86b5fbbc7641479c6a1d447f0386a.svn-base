<script type="text/javascript" th:src="@{https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js}"></script>
<script type="text/javascript" th:src="@{/lib/layui/layui.js}"></script>
<script type="text/javascript" th:src="@{/common/js/tools.js}"></script>
<script>
    layui.use(['layer', 'form', 'table', 'laydate'], function () {
        var layer = layui.layer,
            $ = layui.jquery,
            laydate = layui.laydate,
            date = new Date(),
            form = layui.form,
            table = layui.table,
            t1,
            t2;                  //表格数据变量
        form.render();
        laydate.render({
            elem: '#forecast_time'    //月份选择
            , type: 'month'
            , format: 'yyyy-MM' //可任意组合
            , value: new Date(date.getFullYear(), date.getMonth() + 1)
            , isInitValue: true //是否允许填充初始值，默认为 true
        });
        //资料起止年份
        laydate.render({
            elem: '#time_range'
            , type: 'year'
            , range: true
        });
        //筛选因子的表格
        //全部因子
        t1 = {
            elem: '#tb1',
            url: '/data/wat',//130项气候因子传递接口
            method: 'get',
            initSort: {field: 'id', type: 'asc'},
            page: {
                //支持传入 laypage 组件的所有参数（某些参数除外，如：jump/elem） - 详见文档
                layout: ['limit', 'count', 'prev', 'page', 'next', 'skip'], //自定义分页布局
                //,curr: 5 //设定初始在第 5 页
                groups: 3, //只显示 《1、2、3》  3个连续页码
                first: "首页", //显示首页
                last: "尾页", //显示尾页
                limit: 10,
                limits: [10, 30, 60, 130]
            },
            cols: [
                [
                    {title: '全部因子', colspan: '4'}
                ],
                [
                    {type: 'checkbox', fixed: 'left'},
                    {field: 'id', title: '因子编号', width: '', sort: true},
                    {field: 'climateNameZhcn', title: '因子名称', width: ''},
                    {field: 'climateNameEn', title: '相关系数', width: ''}
                ]
            ]
        };
        //选中的因子
        t2 = {
            elem: '#tb2',
            url: '/data',//130项气候因子传递接口
            method: 'get',
            initSort: {field: 'id', type: 'asc'},
            page: {
                //支持传入 laypage 组件的所有参数（某些参数除外，如：jump/elem） - 详见文档
                layout: ['limit', 'count', 'prev', 'page', 'next', 'skip'], //自定义分页布局
                //,curr: 5 //设定初始在第 5 页
                groups: 3, //只显示 《1、2、3》  3个连续页码
                first: "首页", //显示首页
                last: "尾页", //显示尾页
                limit: 10,
                limits: [10, 30, 60, 130]
            },
            cols: [
                [
                    {title: '选中因子', colspan: '5'}
                ],
                [
                    {type: 'checkbox', fixed: 'left'},
                    {field: 'id', title: '序号', width: '', sort: true},
                    {field: 'ids', title: '因子编号', width: ''},
                    {field: 'climateNameZhcn', title: '因子名称', width: ''},
                    {field: 'climateNameEn', title: '相关系数', width: ''}
                ]
            ]
        };
        table.render(t1);
        table.render(t2);

        //监听指定开关-误差自回归校正开关
        form.on('switch(switchTest)', function (data) {
            //do sth
        });


    });
</script>
</body>
</html>