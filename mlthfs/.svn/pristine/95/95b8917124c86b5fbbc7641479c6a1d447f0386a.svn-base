<fieldset class="layui-elem-field">
    <legend>提交参数</legend>
    <form class="layui-form" style="text-align: center">
        <div class="layui-inline" style="margin-left: 15px">
            <a class="layui-btn  " lay-submit="" lay-filter="searchForm">创建模型</a>
        </div>
    </form>
